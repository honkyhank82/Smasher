    "@react-navigation/stack": "6.3.20",
    "@sentry/react-native": "5.15.0",
    "@shopify/flash-list": "1.6.4",
    "axios": "1.6.2",
    "expo": "~50.0.0",
    "expo-application": "~5.9.0",
    "expo-av": "~13.10.0",
    "expo-constants": "~15.0.0",
    "expo-device": "~5.9.0",
    "expo-image-picker": "~14.7.0",
    "expo-linking": "~6.2.2",
    "expo-location": "~16.1.0",
